{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "scw/machine-block",
	"version": "1.0.0",
	"title": "Machine Description",
	"category": "widgets",
	"icon": "admin-tools",
	"description": "Display machinery information with image, description, usage, safety, and warnings.",
	"supports": {
		"html": false,
		"align": true
	},
	"attributes": {
		"images": {
			"type": "array",
			"default": []
		}
	},
	"textdomain": "scw-machine-block",
    "editorScript": "file:./build/index.js",
    "editorStyle": "file:./build/index.css",
    "style": "file:./build/style-index.css",
    "viewScript": "file:./build/view.js"
}
